<% content_for :title, t("activerecord.models.tenant.other") %>

<h1><%= t("activerecord.models.tenant.other") %></h1>

<div id="tenants">
  <% @tenants.each do |tenant| %>
    <%= render tenant %>
    <p>
      <%= link_to t("helpers.show", model: t("activerecord.models.tenant.one")), [@company, tenant], class: "btn btn-outline-primary" %>
    </p>
  <% end %>
</div>

<%= link_to t("helpers.new", model: t("activerecord.models.tenant.one")), new_company_tenant_path(@company), class: "btn btn-outline-primary" %>